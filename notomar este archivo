        "# Variables numéricas + categóricas dummies\n",
        "X = df[[\"NA_Sales\", \"EU_Sales\", \"JP_Sales\", \"Other_Sales\", \"Genre\", \"Platform\"]]\n",
        "X = pd.get_dummies(X, columns=[\"Genre\", \"Platform\"], drop_first=True)\n",
        "y = df[\"Exito\"]\n",
        "\n"
      ],
      "metadata": {
        "id": "xq9KX0QTtkdm"
