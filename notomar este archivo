        "plt.figure(figsize=(10,8))\n",
        "sns.heatmap(df[[\"NA_Sales\", \"EU_Sales\", \"JP_Sales\", \"Other_Sales\", \"Global_Sales\"]].corr(), annot=True, cmap=\"YlGnBu\")\n",
        "plt.title(\"Matriz de Correlación entre Ventas\")\n",
        "plt.show()\n"
      ],
      "metadata": {
        "id": "ZXD9MJmcrqkR",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 704
        },
        "outputId": "fbc1aa3e-0132-40f1-d6c6-111b281e5827"
      },
      "execution_count": 44,
      "outputs": [
        {
          "output_type": "display_data",
          "data": {
            "text/plain": [
